<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
    <shape>
        <solid
            android:color="#066B1F" />
        <corners
            android:radius="6dp" />
    </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="#066B1F"
                android:endColor="#005105"
                android:angle="270" />
            <corners
                android:radius="6dp" />

        </shape>
    </item>


</selector>